{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "simple webpack config",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --open",
    "dev_win": "set NODE_ENV=development&&webpack-dev-server --config webpack.config.js --open",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build_win": "set NODE_ENV=production&&webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimitriskot/webpack-template.git"
  },
  "author": "dimitriskot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimitriskot/webpack-template/issues"
  },
  "homepage": "https://github.com/dimitriskot/webpack-template#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.4",
    "path": "0.12.7",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  }
}
